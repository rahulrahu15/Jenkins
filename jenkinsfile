pipeline {
  agent any

  stages {

    stage('Checkout') {
      steps {
        git 'https://github.com/rahulrahu15/Jenkins.git'
      }
    }

    stage('Build') {
      steps {
        sh 'echo "Building project..."'
      }
    }

    stage('Test') {
      steps {
        sh 'echo "Running tests..."'
      }
    }

    stage('Deploy') {
      steps {
        sh '''
          cp -r * /var/www/html/
          echo "Deployment completed"
        '''
      }
    }

  }
}
